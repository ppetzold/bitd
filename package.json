{
  "name": "bitd",
  "version": "0.2.1",
  "description": "decentralized autonomous database over bitcoin",
  "main": "./dist/index.js",
  "author": "21 Century Motor Company",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "start": "npm run build && node ./dist/index.js",
    "test": "mocha \"dist/test/**/*.test.js\" --exit --timeout 5000 --require source-map-support/register",
    "lint": "npm run tslint && npm run prettier",
    "tslint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc --write --list-different \"**/{*.{ts,json,js,md,yml},*.*.yml}\""
  },
  "dependencies": {
    "bcode": "0.0.6",
    "bigjq": "0.0.15",
    "bitcoind-rpc": "^0.7.2",
    "dotenv": "^6.0.0",
    "iconv-lite": "^0.4.24",
    "ip": "^1.1.5",
    "level": "^4.0.0",
    "mingo": "^2.3.3",
    "mongodb": "^3.1.4",
    "p-limit": "^2.2.0",
    "p-queue": "^4.0.0",
    "tna": "0.1.2",
    "traverse": "^0.6.6",
    "zeromq": "^4.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/iconv-lite": "0.0.1",
    "@types/ip": "^1.1.0",
    "@types/mongodb": "^3.1.22",
    "@types/node": "^11.13.0",
    "@types/traverse": "^0.6.32",
    "@types/zeromq": "^4.6.2",
    "eslint": "^5.6.1",
    "prettier": "^1.16.4",
    "source-map-support": "^0.5.11",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  }
}
